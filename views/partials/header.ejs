<html>
    <head>
        <title></title>
    </head>
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/style.css">
    
    <body>
            <nav class="navbar navbar-default">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <a href="/" class="navbar-brand">Yelp Camp</a>
                        </div>
                        <div class="navbar-header">
                                <a href="/campgrounds/about" class="navbar-brand">About</a>
                        </div>
                
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav navbar-right">
                                
                                <% if(!currentUser) {%>
                                    <li><a href="/campgrounds/login">Login</a></li>
                                    <li><a href="/campgrounds/register">Sign Up</a></li>
                                <% } else { %>
                                    <li><a href="#">Logged in as <%= currentUser.username %></a></li>
                                    <li><a href="/campgrounds/logout">Logout</a></li>
                                <% } %>
                            </ul>
                        </div>
                
                
                    </div>
                
                </nav>
        
                <div class="container">
                    <% if(error && error.length > 0) {%>
                    <div class="alert alert-danger" role="alert">
                        <p><%= error %></p>
                    </div>
                    <% } %>

                    <% if(success && success.length > 0) {%>
                    <div class="alert alert-success" role="alert">
                        <p><%= success %></p>
                    </div>
                    <%}%>
                </div>
               